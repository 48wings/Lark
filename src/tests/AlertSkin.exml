<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="http://ns.egret.com/swan" xmlns:w="http://ns.egret.com/wing"
        minHeight="230" minWidth="470" maxWidth="710">
    <s:Group left="6" right="6" height="50" id="moveArea">
        <s:Label id="titleDisplay" fontFamily="Tahoma" fontSize="26"
                 textColor="0x727070" left="5" right="5" verticalCenter="0"
                 minHeight="28" verticalAlign="middle" textAlign="center" text="titleDisplay"/>
    </s:Group>
    <s:Label id="contentDisplay" fontFamily="Tahoma" fontSize="22"
             textColor="0x727070" left="1" right="1" top="36" bottom="45"
             verticalAlign="middle" textAlign="center" text="contentDisplay">
    </s:Label>
    <s:Button label="按钮">
        <s:Skin states="up,over,down,disabled" xmlns:e="http://ns.egret-labs.org/egret" xmlns:w="http://ns.egret-labs.org/wing">
            <s:Image source="image/button_up.png" includeIn="up" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <s:Image source="image/button_over.png" includeIn="over" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <s:Image source="image/button_down.png" includeIn="down" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <s:Image source="image/button_disabled.png" includeIn="disabled" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <s:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" left="20" right="20" top="10" bottom="10"/>
            <s:Image id="iconDisplay" horizontalCenter="0" verticalCenter="0"/>
        </s:Skin>
    </s:Button>
</s:Skin>