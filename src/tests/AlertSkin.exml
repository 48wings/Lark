<?xml version="1.0" encoding="utf-8"?>
<e:Skin xmlns:e="http://ns.egret-labs.org/egret" xmlns:w="http://ns.egret-labs.org/wing"
        minHeight="230" minWidth="470" maxWidth="710">
    <e:Group left="6" right="6" height="50" id="moveArea">
        <e:Label id="titleDisplay" fontFamily="Tahoma" fontSize="26"
                 textColor="0x727070" left="5" right="5" verticalCenter="0"
                 minHeight="28" verticalAlign="middle" horizontalAlign="center" text="titleDisplay"/>
    </e:Group>
    <e:Label id="contentDisplay" fontFamily="Tahoma" fontSize="22"
             textColor="0x727070" left="1" right="1" top="36" bottom="45"
             verticalAlign="middle" horizontalAlign="center" text="contentDisplay">
    </e:Label>
    <e:Button label="按钮">
        <e:Skin>
            <e:states>
                <e:State name="up"/>
                <e:State name="over"/>
                <e:State name="down"/>
                <e:State name="disabled"/>
            </e:states>
            <e:Image source="image/button_up.png" includeIn="up" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <e:Image source="image/button_over.png" includeIn="over" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <e:Image source="image/button_down.png" includeIn="down" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <e:Image source="image/button_disabled.png" includeIn="disabled" width="100%" height="100%" scale9Grid="5,5,63,16"/>
            <e:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" left="20" right="20" top="10" bottom="10"/>
            <e:Image id="iconDisplay" horizontalCenter="0" verticalCenter="0"/>
        </e:Skin>
    </e:Button>
</e:Skin>