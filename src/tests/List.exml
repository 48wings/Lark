<s:Skin xmlns:s="http://ns.egret.com/swan" xmlns:w="http://ns.egret.com/wing"
        class="swan.ScrollerTest">
    <s:Scroller width="200" height="200" horizontalCenter="0" verticalCenter="0">
        <s:Skin>
            <s:VScrollBar height="100%" id="verticalScrollBar" right="0">
                <s:Skin>
                    <s:Image source="image/button_disabled.png" width="10" height="100%" scale9Grid="5,5,63,16"/>
                    <s:Image source="image/button_up.png" id="thumb" width="10" height="30" scale9Grid="5,5,63,16"/>
                </s:Skin>
            </s:VScrollBar>
            <s:HScrollBar width="100%" id="horizontalScrollBar" bottom="0">
                <s:Skin>
                    <s:Image source="image/button_disabled.png" height="10" width="100%" scale9Grid="5,5,63,16"/>
                    <s:Image source="image/button_up.png" id="thumb" height="10" width="30" scale9Grid="5,5,63,16"/>
                </s:Skin>
            </s:HScrollBar>
        </s:Skin>
        <s:List id="list">
            <s:itemRenderer>
                <s:ItemRenderer states="up,down" height="500">
                    <s:Image source="image/button_up.png" includeIn="up" width="100%" height="100%"
                             scale9Grid="5,5,63,16"/>
                    <s:Image source="image/button_down.png" includeIn="down" width="100%" height="100%"
                             scale9Grid="5,5,63,16"/>
                    <s:Label id="labelDisplay" text="{data.label}" horizontalCenter="0" verticalCenter="0" left="20" right="20" top="10"
                             bottom="10"/>
                </s:ItemRenderer>
            </s:itemRenderer>
            <s:ArrayCollection>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
                <s:Object label="测试文本"/>
            </s:ArrayCollection>
            <s:layout>
                <s:HorizontalLayout/>
            </s:layout>
        </s:List>
    </s:Scroller>

</s:Skin>
