<s:Scroller xmlns:s="http://ns.egret.com/swan" xmlns:w="http://ns.egret.com/wing"
            class="swan.ScrollerTest" width="200" height="200" horizontalCenter="0" verticalCenter="0">
    <s:Skin>
        <s:VScrollBar height="100%" id="verticalScrollBar" right="0">
            <s:Skin>
                <s:Image source="image/button_disabled.png" width="10" height="100%" scale9Grid="5,5,63,16"/>
                <s:Image source="image/button_up.png" id="thumb" width="10" height="30" scale9Grid="5,5,63,16"/>
            </s:Skin>
        </s:VScrollBar>
        <s:HScrollBar width="100%" id="horizontalScrollBar" bottom="0">
            <s:Skin>
                <s:Image source="image/button_disabled.png" height="10" width="100%" scale9Grid="5,5,63,16"/>
                <s:Image source="image/button_up.png" id="thumb" height="10" width="30" scale9Grid="5,5,63,16"/>
            </s:Skin>
        </s:HScrollBar>
    </s:Skin>
    <s:Group touchThrough="true">
        <s:Button label="按钮" touchEnabled="false">
            <s:Skin height="500" width="500" states="up,over,down,disabled">
                <s:Image source="image/button_up.png" includeIn="up" width="100%" height="100%" scale9Grid="5,5,63,16"/>
                <s:Image source="image/button_over.png" includeIn="over" width="100%" height="100%" scale9Grid="5,5,63,16"/>
                <s:Image source="image/button_down.png" includeIn="down" width="100%" height="100%" scale9Grid="5,5,63,16"/>
                <s:Image source="image/button_disabled.png" includeIn="disabled" width="100%" height="100%" scale9Grid="5,5,63,16"/>
                <s:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" left="20" right="20" top="10" bottom="10"/>
                <s:Image id="iconDisplay" horizontalCenter="0" verticalCenter="0"/>
            </s:Skin>
        </s:Button>
    </s:Group>

</s:Scroller>